<?xml version="1.0"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IClientService1">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_IClientService">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_IClientService2">
                    <security mode="Transport" />
                </binding>
            </basicHttpBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_IClientService1">
                    <security mode="Transport">
                        <transport clientCredentialType="None" />
                    </security>
                </binding>
                <binding name="WSHttpBinding_IClientService">
                    <security mode="Transport">
                        <transport clientCredentialType="None" />
                    </security>
                </binding>
                <binding name="WSHttpBinding_IClientService2">
                    <security mode="Transport">
                        <transport clientCredentialType="None" />
                    </security>
                </binding>
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="https://sandbox.paymentsgateway.net/ws/Client.svc"
                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IClientService"
                contract="SandboxClientV1WS.IClientService" name="WSHttpBinding_IClientService" />
            <endpoint address="https://sandbox.paymentsgateway.net/ws/Client.svc/basic"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IClientService"
                contract="SandboxClientV1WS.IClientService" name="BasicHttpBinding_IClientService" />
            <endpoint address="https://ws.paymentsgateway.net/Service/v1/Client.svc"
                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IClientService2"
                contract="LiveClientV1WS.IClientService" name="WSHttpBinding_IClientService1" />
            <endpoint address="https://ws.paymentsgateway.net/Service/v1/Client.svc/basic"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IClientService2"
                contract="LiveClientV1WS.IClientService" name="BasicHttpBinding_IClientService1" />
        </client>
    </system.serviceModel>
</configuration>
